<button class="btn" type="primary" bindtap="onTap">{{langBtn}}</button>
<image src='/images/header.jpg' mode="widthFix"></image>
<view class="content">
    <view class="title">
        {{title}}
    </view>
    <view wx:for="{{titles}}">
        <text>{{item}}</text>
    </view>
    <view class="topic">
        {{topic}}
    </view>
    <view wx:for="{{topics}}">
        <text>{{item}}</text>
    </view>
</view>